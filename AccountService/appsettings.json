{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangifre": "Information"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/modulbank/protocol/openid-connect/auth"
  },
  "Authentication": {
    "Issuer": "http://localhost:8080/realms/modulbank",
    "Audience": "account-service",
    "MetadataAddress": "http://localhost:8080/realms/modulbank/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "Database": {
    "ConnectionString": "Host=localhost;Port=5432;Database=modulbankdb;Username=modulbankuser;Password=modulbankpass",
    "MustMigrate": true
  },
  "RabbitMq": {
    "Host": "localhost",
    "VirtualHost": "/",
    "User": "guest",
    "Password": "guest",
    "ExchangeName": "account.events"
  },
  "ConnectionStrings": {
    "Hangfire": "Host=localhost;Port=5432;Database=hangfiredb;Username=modulbankuser;Password=modulbankpass"
  }
}
